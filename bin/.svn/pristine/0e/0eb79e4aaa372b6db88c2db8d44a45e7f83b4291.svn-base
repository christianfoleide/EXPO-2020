package com.expo2020.repositories;

import com.expo2020.models.Stand;
import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import java.util.List;
import java.util.Optional;


import java.util.List;

/***
 * @author Christian Foleide
 *  Interface som definerer CRUD-operasjoner med Stand-objekt på databasen.
 *  Metodene trengs ikke implementeres for å bruke, så langt navnekonvensjon følges.
 *  Se https://docs.spring.io/spring-data/commons/docs/current/api/org/springframework/data/repository/CrudRepository.html
 */

@Repository
public interface StandRepository extends CrudRepository<Stand, Long> {

    Optional<Stand> findById(int id);

    List<Stand> findAll();

}